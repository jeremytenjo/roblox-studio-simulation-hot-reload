{
  "name": "roblox-studio-simulation-hot-reload",
  "displayName": "test+",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.0"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "robloxAutoTest.runOrRestart",
        "title": "Roblox: Run or Restart Test"
      }
    ],
    "keybindings": [
      {
        "command": "robloxAutoTest.runOrRestart",
        "key": "f2",
        "when": "true"
      }
    ]
  },
  "scripts": {
    "dev": "tsc -watch -p ./",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "test": "echo \"no tests\""
  },
  "devDependencies": {
    "@types/mocha": "10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "1.107.0",
    "@types/ws": "8.18.1",
    "@vscode/test-cli": "0.0.12",
    "@vscode/test-electron": "2.5.2",
    "esbuild": "0.27.1",
    "eslint": "9.39.1",
    "npm-run-all": "4.1.5",
    "typescript": "5.9.3",
    "typescript-eslint": "8.48.1"
  },
  "dependencies": {
    "ws": "8.18.3"
  }
}
