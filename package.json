{
  "name": "roblox-studio-simulation-hot-reload",
  "description": "VS Code extension for hot reloading Roblox Studio simulations and running tests",
  "version": "0.0.1",
  "publisher": "tenjojeremy",
  "type": "module",
  "engines": {
    "vscode": "^1.107.0"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "robloxAutoTest.runOrRestart",
        "title": "Roblox: Run or Restart Test"
      }
    ],
    "keybindings": [
      {
        "command": "robloxAutoTest.runOrRestart",
        "key": "f2",
        "when": "true"
      }
    ]
  },
  "scripts": {
    "dev": "tsc -watch -p ./",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "test": "echo \"no tests\"",
    "prettify": "prettier --write .",
    "tests:lint": "eslint .",
    "tests:ts-types": "tsgo --noEmit"
  },
  "devDependencies": {
    "@types/mocha": "10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "1.107.0",
    "@types/ws": "8.18.1",
    "@typescript/native-preview": "7.0.0-dev.20251210.1",
    "@vscode/test-cli": "0.0.12",
    "@vscode/test-electron": "2.5.2",
    "eslint": "9.39.1",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-require-js-extension": "0.0.5",
    "npm-run-all": "4.1.5",
    "prettier": "3.7.4",
    "typescript": "5.9.3",
    "typescript-eslint": "8.48.1"
  },
  "dependencies": {
    "eslint-config-prettier": "10.1.8",
    "ws": "8.18.3"
  }
}
